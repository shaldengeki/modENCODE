<h1>Latest updates</h1>

<table>
  <tr>
    <th>User</th>
    <th>Status</th>
    <th>Reagent</th>
    <th>Time</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @status_updates.each do |status_update| %>
  <tr>
    <td><%= status_update.user %></td>
    <td><%= status_update.status %></td>
    <td><%= status_update.reagent %></td>
    <td><%= status_update.time %></td>
    <td><%= status_update.description %></td>
    <td><%= link_to 'Show', status_update %></td>
    <td><%= link_to 'Edit', edit_status_update_path(status_update) %></td>
    <td><%= link_to 'Destroy', status_update, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

